{
    "env": {},
    "configurations": [
        {
            "name": "Mac",
            "compilerPath": "/usr/bin/clang++",
            "intelliSenseMode": "macos-clang-arm64",
            "defines": [
                "${myDefines}"
            ],
            "cppStandard": "c++17",
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ]
            },
            "includePath": [
                "${default}"
            ]
        }
    ],
    "version": 4,
    "enableConfigurationSquiggles": true
}